{
  "name": "@teejay/api",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "start": "nodemon --watch src --ext ts --exec 'pnpm build:development && pnpm serve | pino-pretty'",
    "serve": "node dist/server.js",
    "prebuild": "rm -rf dist",
    "build": "pnpm build:production",
    "build:development": "pnpm build:esb && pnpm postbuild",
    "build:production": "pnpm prebuild && pnpm build:tsc && pnpm build:esb && pnpm postbuild",
    "build:esb": "esbuild `find src -name '*.ts'` --outdir=dist --format=cjs",
    "build:tsc": "tsc",
    "postbuild": "tsp"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@teejay/prisma-client": "workspace:*",
    "@teejay/typescript-paths": "workspace:*",
    "@trpc/server": "10.0.0-proxy-alpha.59",
    "aws-sdk": "^2.1213.0",
    "bcrypt": "^5.0.1",
    "date-fns": "^2.29.2",
    "dotenv": "^16.0.2",
    "fastify": "^4.5.3",
    "fastify-file-upload": "^4.0.0",
    "jsonwebtoken": "^8.5.1",
    "sharp": "^0.31.0",
    "superjson": "^1.9.1",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@teejay/tsconfig": "workspace:*",
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.7.15",
    "@types/sharp": "^0.30.5",
    "esbuild": "^0.15.5",
    "nodemon": "^2.0.19",
    "pino-pretty": "^9.1.0",
    "typescript": "^4.7.4"
  }
}
