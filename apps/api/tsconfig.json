{
  "extends": "@teejay/tsconfig/base.json",
  "compilerOptions": {
    "composite": false,
    "incremental": false,
    "module": "CommonJS",
    "target": "ES2017",
    "paths": {
      "@/*": ["./src/*"]
    },
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./dist",
    "declaration": true,
    "emitDeclarationOnly": true,
    "skipLibCheck": true
  }
}
